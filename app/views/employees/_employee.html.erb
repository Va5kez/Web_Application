<tr>
  <td><%= employee.id %></td>
  <td><%= link_to employee.name, employee_path(employee.id) %></td>
  <td><%= employee.address %></td>
  <td><%= employee.primary_email %></td>
  <td><%= employee.secondary_email %></td>
  <td><%= employee.birth_at %></td>
  <td><%= employee.start_at %></td>
  <td><%= employee.user %></td>
  <td><%= employee.tipo %></td>
  <td><%= link_to "Borrar", employee,class: "btn btn-default", method: :delete, data: {confirm: "Eliminar Empleado?"} %></td>
</tr>
